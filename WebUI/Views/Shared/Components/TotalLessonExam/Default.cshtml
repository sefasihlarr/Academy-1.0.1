@model TotalLessonExamsModel
<script>
    const xValues = ["Matematik", "Felsefe", "Coğrafya", "Fizik", "Kimya", "Biyoloji", "Tarih", "Edebiyat", "Türkçe"];
    const yValues = [@Model.TotalMatematik,@Model.TotalFelsefe,@Model.TotalCografya,@Model.TotalFizik,@Model.TotalKimya,@Model.TotalBiyoloji,@Model.TotalTarih,@Model.TotalEdebiyat,@Model.TotalTukce];


    new Chart("analize", {
        type: "line",
        data: {
            labels: xValues,
            datasets: [{
                fill: true,
                lineTension: 0,
                backgroundColor: "rgba(0,0,255,0.1)",

                borderColor: "rgba(0,0,255,0.6)",
                data: yValues
            }]
        },
        options: {

            legend: { display: false },
            scales: {
                yAxes: [{ ticks: { min: 0, max: 30 } }],
            }
        }
    });
</script>